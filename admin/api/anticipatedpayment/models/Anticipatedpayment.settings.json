{
  "connection": "default",
  "collectionName": "anticipatedpayment",
  "info": {
    "name": "anticipatedpayment",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "recipientName": {
      "type": "string"
    },
    "recipientEmail": {
      "type": "email"
    },
    "date": {
      "type": "date"
    },
    "description": {
      "type": "text"
    },
    "amount": {
      "type": "integer"
    },
    "comments": {
      "type": "text"
    },
    "user": {
      "model": "user",
      "plugin": "users-permissions"
    },
    "projectManager": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Payed",
        " Unpayed"
      ]
    },
    "sumPayed": {
      "type": "decimal"
    },
    "receiptNumber": {
      "type": "integer"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Cheque",
        " Bank Transfer",
        " Defrayal",
        " Cash"
      ]
    },
    "accountNumber": {
      "type": "integer"
    }
  }
}